<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soecode.lyf.dao.UserLoginDao">
    <select id="queryUserInfoLogin" resultType="UserInfo" parameterType="String">
        SELECT
        *
        FROM
        nice_userlogin
        WHERE
        username= #{username}
    </select>
</mapper>